<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yinnonbar.securedwallet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hi_res_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".Welcome2Activity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".Welcome3Activity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".Welcome4Activity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".AddNewUserActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog.Alert">
        </activity>
        <activity android:name=".AboutActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog.Alert">
        </activity>
        <activity android:name=".WalletActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".AddNewItemActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog.Alert">
        </activity>
        <activity android:name=".EditItemActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog.Alert">
        </activity>
    </application>

</manifest>
